# Production Environment Configuration
# Copy this to .env when deploying to production

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=kaha-prod
DB_PASSWORD=your-production-password
DB_NAME=kaha_hostel_db
DB_DATABASE=kaha_hostel_db

# Application Configuration
APP_PORT=3001
NODE_ENV=production

# JWT Configuration
JWT_SECRET=your-production-secret
JWT_SECRET_TOKEN=your-production-secret

# Logging
LOG_LEVEL=info

# Business Token (replace with production token)
BUSINESS_TOKEN=your-production-business-token

# External API URLs - Production server (api.kaha.com.np)
KAHA_MAIN_API_URL=https://api.kaha.com.np/main/api/v3
KAHA_NOTIFICATION_URL=https://api.kaha.com.np/notifications/api/v3
EXPRESS_NOTIFICATION_URL=https://api.kaha.com.np
NOTIFICATION_SERVER_URL=api.kaha.com.np
